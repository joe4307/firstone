<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.firstone.mapper.UserMapper">

    <select id="selectById" resultType="com.example.firstone.entity.User">
        select *
        from user
        where id = #{id}
    </select>
    <select id="list" resultType="com.example.firstone.entity.User">
        select * from user
    </select>
</mapper>